    {
      "cell_type": "code",
      "source": [
        "iris = sklearn.datasets.load_iris()\n",
        "\n",
        "\n",
        "data1 = pd.DataFrame(data= np.c_[iris['data'], iris['target']],\n",
        "                     columns= iris['feature_names'] + ['target'])\n",
        "\n",
        "X = data1.copy()\n",
        "y = X.pop('target')\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size = 0.4)\n",
        "\n",
        "\n"
      ],
      "metadata": {
        "id": "hpreIwTgqQvK"
